{
  "name": "Fantavega",
  "version": "0.1.0",
  "private": true,
  "packageManager": "pnpm@10.14.0",
  "scripts": {
    "dev": "concurrently \"next dev\" \"tsx watch socket-server.ts\"",
    "dev:next": "next dev",
    "dev:socket": "tsx watch socket-server.ts",
    "build": "next build",
    "start": "next start",
    "start:socket": "node --import tsx socket-server.ts",
    "start:all": "concurrently \"pnpm start\" \"node --import tsx socket-server.ts\"",
    "test": "vitest",
    "test:run": "vitest run",
    "test:ui": "vitest --ui",
    "lint": "next lint",
    "format": "prettier src/ --write",
    "type-check": "tsc --noEmit",
    "socket:dev": "tsx watch socket-server.ts",
    "db:seed": "pnpm exec tsx src/lib/db/seed.ts",
    "db:backup": "pnpm exec tsx src/lib/db/backup.ts",
    "db:migrate": "pnpm exec tsx src/lib/db/migrate.ts",
    "db:reset": "pnpm exec tsx src/lib/db/reset.ts",
    "db:apply-changes": "pnpm exec tsx src/lib/db/apply-changes.ts",
    "link:images": "tsx scripts/link-images.ts"
  },
  "dependencies": {
    "@clerk/nextjs": "^6.23.1",
    "@libsql/client": "^0.15.15",
    "@radix-ui/react-alert-dialog": "^1.1.14",
    "@radix-ui/react-checkbox": "^1.3.2",
    "@radix-ui/react-dialog": "^1.1.14",
    "@radix-ui/react-dropdown-menu": "^2.1.6",
    "@radix-ui/react-label": "^2.1.7",
    "@radix-ui/react-popover": "^1.1.14",
    "@radix-ui/react-progress": "^1.1.7",
    "@radix-ui/react-scroll-area": "^1.2.9",
    "@radix-ui/react-select": "^2.2.5",
    "@radix-ui/react-slot": "^1.1.2",
    "@radix-ui/react-tabs": "^1.1.13",
    "class-variance-authority": "^0.7.1",
    "clsx": "^2.1.1",
    "cmdk": "^1.1.1",
    "date-fns": "^4.1.0",
    "dotenv": "^17.2.3",
    "fs-extra": "^11.3.0",
    "lucide-react": "^0.483.0",
    "next": "15.5.9",
    "next-themes": "^0.4.6",
    "react": "^19.0.0",
    "react-dom": "^19.0.0",
    "socket.io": "^4.8.1",
    "socket.io-client": "^4.8.1",
    "sonner": "^2.0.5",
    "svix": "^1.74.1",
    "tailwind-merge": "^3.0.2",
    "tailwindcss-animate": "^1.0.7",
    "xlsx": "^0.18.5",
    "zod": "^3.25.67"
  },
  "devDependencies": {
    "@testing-library/jest-dom": "^6.8.0",
    "@testing-library/react": "^16.3.0",
    "@trivago/prettier-plugin-sort-imports": "^5.2.0",
    "@types/fs-extra": "^11.0.4",
    "@types/node": "^22",
    "@types/react": "^18",
    "@types/react-dom": "^18",
    "@vitejs/plugin-react": "^5.0.1",
    "babel-plugin-react-compiler": "^1.0.0",
    "concurrently": "^9.2.0",
    "eslint": "^9.17.0",
    "eslint-config-next": "15.4.8",
    "eslint-config-prettier": "^9.1.0",
    "eslint-plugin-check-file": "^2.8.0",
    "jsdom": "^26.1.0",
    "postcss": "^8",
    "prettier": "3.4.2",
    "prettier-plugin-tailwindcss": "^0.6.9",
    "tailwindcss": "^3.4.17",
    "tsx": "^4.19.4",
    "typescript": "^5",
    "typescript-language-server": "^4.4.0",
    "vitest": "^3.2.4"
  },
  "pnpm": {
    "onlyBuiltDependencies": [
      "@clerk/shared",
      "better-sqlite3",
      "esbuild",
      "sharp",
      "unrs-resolver"
    ]
  }
}
